big = 300
small = 40
if big > 10:
    print("big is greater than 10 ",end="\n")
if big > 20:
    print("big is greater than 20 ",end="\n")
if big < 301:
    print("big is less than 301 ",end="\n")
if big==small:
    print(" big and small are equal ",end="\n")
else:
    print("big is greater than small",end="\n")
